        <Label Content="端口号：" HorizontalAlignment="Left" Margin="131,70,0,0" VerticalAlignment="Top"/>
        <TextBox Name="textbox2" HorizontalAlignment="Left" Height="23" Margin="250,70,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="200"/>
        <Button Click="button_click1" Content="建立连接" HorizontalAlignment="Left" Margin="500,50,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox Name="textbox3" HorizontalAlignment="Left" Height="23" Margin="510,110,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="200"/>
        <Label Content="KEYc,v:" HorizontalAlignment="Left" Margin="440,110,0,0" VerticalAlignment="Top"/>
        <Label Content="msg5:" HorizontalAlignment="Left" Margin="131,190,0,0" VerticalAlignment="Top"/>
