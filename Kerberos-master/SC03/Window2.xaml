        <TextBox Name="textbox5" HorizontalAlignment="Left" Height="23" Margin="200,110,0,0" TextWrapping="Wrap"   VerticalAlignment="Top" Width="200"/>
        <Label Content="tkt_v:" HorizontalAlignment="Left" Margin="131,110,0,0" VerticalAlignment="Top"/>
        <TextBox Name="textbox6" HorizontalAlignment="Left" Height="23" Margin="200,150,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="300"/>
        <Label Content="Aut:" HorizontalAlignment="Left" Margin="131,150,0,0" VerticalAlignment="Top"/>
        <Button Click="button_click3" Content="生成Aut" HorizontalAlignment="Left" Margin="543,150,0,0" VerticalAlignment="Top" Width="75"/>
        <Label Content="反馈信息" HorizontalAlignment="Left" Margin="131,240,0,0" VerticalAlignment="Top"/>
        <TextBox Name="textbox7" HorizontalAlignment="Left" Height="23" Margin="230,240,0,0" TextWrapping="Wrap"   VerticalAlignment="Top" Width="400"/>
        
    </Grid>
</Window>
